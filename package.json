{
  "name": "clara-xlsx-diff",
  "version": "0.1.6",
  "description": "XLSX comparison tool using Clara Rules and EAV triples",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "target/main-simple.js",
    "types.d.ts",
    "utils.ts",
    "utils.js",
    "example-usage.ts",
    "example-usage.js",
    "README.md"
  ],
  "bin": {},
  "scripts": {
    "build": "npm run build:clara-simple && npm run build:ts",
    "build:ts": "npx tsc utils.ts example-usage.ts",
    "build:lib": "npx shadow-cljs release lib",
    "build:demo": "npx shadow-cljs release demo",
    "build:browser": "npx shadow-cljs compile browser",
    "build:clara-rules": "lein cljsbuild once main",
    "build:clara-simple": "lein cljsbuild once simple",
    "build:all": "npm run build:lib && npm run build:clara-simple",
    "prepublishOnly": "npm run build",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "watch:lib": "npx shadow-cljs watch lib",
    "dev": "npx shadow-cljs watch dev",
    "dev:browser": "npx shadow-cljs watch browser",
    "demo": "node dist/demo.js",
    "example": "node example.js",
    "test:cljs": "npx shadow-cljs compile test && node dist/test.js",
    "test:clara-rules": "lein cljsbuild once test && node target/test.js",
    "test:clj": "clojure -M:test",
    "test": "npm run test:clara-rules",
    "test:all": "npm run test:clj && npm run test:clara-rules && npm run test:cljs",
    "serve": "npx shadow-cljs watch dev",
    "repl": "npx shadow-cljs cljs-repl lib",
    "clean": "rm -rf dist/* target/*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "shadow-cljs": "^2.28.23",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "xlsx",
    "excel",
    "diff",
    "comparison",
    "clara-rules",
    "clojure"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "EPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcouk/clara-xlsx-diff.git"
  },
  "homepage": "https://github.com/jcouk/clara-xlsx-diff#readme",
  "bugs": {
    "url": "https://github.com/jcouk/clara-xlsx-diff/issues"
  }
}
